  -unit
  -interval
  ~create() 
}
class "DoorOpenedRefrigeratorFactory" as DoorOpenedRefrigeratorFactory << (C, DarkSeaGreen) >> { 
  -unit
  -interval
  ~create() 
}
class "NormalRefrigeratorFactory" as NormalRefrigeratorFactory << (C, DarkSeaGreen) >> { 
  -unit
  -interval
  ~create() 
}
